{
  "name": "webtemplate",
  "version": "1.0.0",
  "description": "Template for a website",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run compile:sass\" \"npm run serve\"",
    "compile:sass": "node-sass sass/main.scss css/style.css -w",
    "serve": "lite-server"
  },
  "author": "Tim New",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.1.0",
    "lite-server": "^2.4.0",
    "node-sass": "^4.11.0"
  }
}
